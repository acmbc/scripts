local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/preztel/AzureLibrary/master/uilib.lua", true))()

local LuckTab = Library:CreateTab("farm") 


LuckTab:CreateToggle("active", function(arg)
if arg then
	_G.luck1 = true -- put false to stop
while _G.luck1 == true and task.wait() do
game.Players.LocalPlayer.Character.HumanoidRootPart:PivotTo(workspace.1.CFrame)
wait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart:PivotTo(workspace.2.CFrame)
waait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart:PivotTo(workspace.3.CFrame)
wait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart:PivotTo(workspace.4.CFrame)
wait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart:PivotTo(workspace.5.CFrame)
wait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart:PivotTo(workspace.6.CFrame)
end
	else
	_G.luck1 = false
	end
end)
